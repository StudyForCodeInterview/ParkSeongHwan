#include <iostream>
#include <vector>
#include <queue>
#include <stack>
#include <memory.h>
#pragma warning (disable:4996)
using namespace std;

int arr[10];

int main()
{
	std::ios::sync_with_stdio(false);
	std::cin.tie(nullptr);
	std::cout.tie(nullptr);

	int n, k;
	cin >> n >> k;

	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}

	int count = 0;

	for (int i = n - 1; i >= 0; i--) {
		while (arr[i] <= k) {
			k -= arr[i];
			count++;
		}

		if (k == 0)
			break;
	}



	cout << count;
}
